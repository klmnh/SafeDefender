<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/iv_processIcon"
        android:layout_alignParentLeft="true"
        android:background="@drawable/recyclebin"
        android:layout_centerVertical="true"
        android:layout_width="45dp"
        android:layout_height="45dp" />

    <TextView
        android:id="@+id/tv_processName"
        android:text="应用名称"
        android:textColor="#000"
        android:textSize="18sp"
        android:layout_toRightOf="@id/iv_processIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tv_processPath"
        android:text="拦截号码"
        android:textColor="#000"
        android:textSize="18sp"
        android:layout_below="@id/tv_processName"
        android:layout_toRightOf="@id/iv_processIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <CheckBox
        android:id="@+id/cb_processCheck"
        android:clickable="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:layout_alignParentRight="true"
        android:gravity="center_vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</RelativeLayout>